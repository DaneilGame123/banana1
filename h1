local function PXIVXRZ_fake_script() -- Beans.LocalScript 
	local script = Instance.new('LocalScript', Beans)

	game:GetService("UserInputService").InputBegan:Connect(function(Input)
		if Input.KeyCode == Enum.KeyCode["RightShift"] then
			if script.Parent.Enabled == true then
				script.Parent.Enabled = false
			elseif script.Parent.Enabled == false then
				script.Parent.Enabled = true
			end
		end
	end)
end
coroutine.wrap(PXIVXRZ_fake_script)()
local function OTQRG_fake_script() -- TextButton1.GuiClick_Script 
	local script = Instance.new('LocalScript', TextButton1)

	function OnClicked() 
		game.Workspace.Gravity = script.Parent.Parent.TextBox1.Text
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end
coroutine.wrap(OTQRG_fake_script)()
local function YLOEQHZ_fake_script() -- TextButton2.LocalScript 
	local script = Instance.new('LocalScript', TextButton2)

	function OnClicked() 
		local Player = game.Players.LocalPlayer
		local Character = Player.Character
		Character.Humanoid.WalkSpeed = script.Parent.Parent.TextBox2.Text
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end
coroutine.wrap(YLOEQHZ_fake_script)()
local function RSSMCW_fake_script() -- ColorCorrectionDisable.GuiClick_Script 
	local script = Instance.new('LocalScript', ColorCorrectionDisable)

	function OnClicked() 
		game:GetService("Lighting").ColorCorrection:Destroy()
		wait()
		game:GetService("Lighting").ColorCorrection:Destroy()
	end
		script.Parent.MouseButton1Down:connect(OnClicked)
end
coroutine.wrap(RSSMCW_fake_script)()
