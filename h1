local function QHNFT_fake_script() -- Virusinfect.GuiClick_Script 
	local script = Instance.new('LocalScript', Virusinfect)

	
	function OnClicked()
		script.Parent.Visible = false
		local teleport = Instance.new("Part")
		teleport.Parent = workspace
		teleport.Position = game.Players.LocalPlayer.Character.Head.Position
		teleport.Name = "teleportE"
		teleport.Anchored = true
		teleport.Size = Vector3.new(1, 1, 1)
		teleport.Transparency = 0.3
		teleport.BrickColor = BrickColor.new(255, 0, 0)
		teleport.CanCollide = false
		wait(1)
		game.Players.LocalPlayer.Character:MoveTo(game:GetService("Workspace").StaticSCPFolder["SCP-409"].Touch.Position)
		wait(3)
		game.Players.LocalPlayer.Character:MoveTo(teleport.Position)
		wait(5)
		teleport:Destroy()
		script.Parent.Visible = true
	end
		script.Parent.MouseButton1Down:connect(OnClicked)
end
coroutine.wrap(QHNFT_fake_script)()
